					{@Model.Shah.AtStockpile.ToString("N0", System.Globalization.CultureInfo.CurrentCulture) }
				else
				{ <span>0</span>}</div>
					<span class="caption">@Resources.ResourceMnemonic.Tonns</span>
					<div class="clearfix"></div>
				</div>
				<div class="result">

					@{ var shahwarning1 = Html.Raw(@"<i class='green'></i>");
