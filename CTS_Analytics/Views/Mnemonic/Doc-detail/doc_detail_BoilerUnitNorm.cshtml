@model IEnumerable<CTS_Analytics.Models.Mnemonic.Doc_detail.BoilerNorm>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.PageName = Resources.ResourceMnemonic.Thesaurus;
    ViewBag.PageSubName = @Resources.ResourceMnemonic.Skip_many;
}

<p class="subname"><span>@Resources.ResourceMnemonic.Thesaurus</span></p>
<div class="main">
    <h2>@Resources.ResourceMnemonic.BoilerNorm</h2>
    <table class="table table-striped table-hover sortable">
        <thead>
            <tr>
                <th></th>
                <th>@Resources.ResourceMnemonic.BoilerConsumptionNorm</th>
            </tr>
        </thead>
        @if (Model != null)
        {
            foreach (var m in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => m.Location)</td>
                    <td>@Html.DisplayFor(modelItem => m.ConsumptionNorm)</td>
                </tr>
            }
        }
    </table>
</div>